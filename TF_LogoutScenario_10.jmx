              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">def rand = new Random()

String status = vars.get(&quot;status&quot;) // из vars достается значение status
Integer status_integer

switch(status){ // присваинвание числового значения статусу
    case &quot;Open&quot;:
        status_integer = 1
        break
    case &quot;Reopened&quot;:
        status_integer = 2
        break
    case &quot;Resolved&quot;:
        status_integer = 3
        break
    case &quot;Closed&quot;:
        status_integer = 4
        break
    case &quot;Duplicate&quot;:
        status_integer = 5
        break
    default:
        AssertionResult.setFailure(true)
}
Integer new_status = status_integer;
while(new_status == status_integer){ // рандомится до тех пор, пока новый и старый статусы не будут разными 
	new_status = rand.nextInt(4) + 1;
}

vars.put(&quot;new_status&quot;, String.valueOf(new_status)); // отправка нового статуса в vars</stringProp>

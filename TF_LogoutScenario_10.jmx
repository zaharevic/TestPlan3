String jsonResponse = prev.getResponseDataAsString() // достается ответ на запрос

def jsonSlurper = new JsonSlurper()
def response = jsonSlurper.parseText(jsonResponse) // ответ парсится в json

def data = response.data

int minPriority = Integer.MAX_VALUE // просто затычка, чтоб любое число точно было меньше максимального значения int
List&lt;Map&gt; minPriorityItems = []

data.each { item -&gt;
    if (item.priority &lt; minPriority) {
        minPriority = item.priority // достается число обозначающее минимальный приоритет из всех
    }
}

minPriorityItems = data.findAll { it.priority == minPriority } // в массив собираются все тикеты с минимальным приоритетом

if (minPriorityItems.size() &gt; 0) {
    Random rand = new Random()
    def randomItem = minPriorityItems.get(rand.nextInt(minPriorityItems.size())) // достается рандомный тикеты
    int selectedId = randomItem.id // сохраняется id титкета

    vars.put(&quot;rand_id&quot;, selectedId.toString()) //сохраняется id тикета в vars
}
